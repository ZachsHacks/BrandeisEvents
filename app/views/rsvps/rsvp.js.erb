if(document.getElementById("user_events")!=null){
  $("#user_events").html("<%=escape_javascript(render(:partial => 'events/user_events'))%>")
} else {
  $("#rsvp_buttons_<%=@event_id%>").html("<%=escape_javascript(render(:partial => 'events/rsvp_button', locals: {event: @event}))%>")
  $("#rsvp_list").html("<%=escape_javascript(render(:partial => 'events/display_rsvps', locals: {event: @event}))%>")
}
