<p id="notice"><%= notice %></p>

<div class="container">
    <% provide(:title, @user.first_name) %>

    <div class="top-thumbnail profile-banner text-center">
        <%= image_tag "your-profile.png", class:"img-responsive" %>
        <div class="top-caption">
            <h1>
                <%= @user.first_name %>'s Events
            </h1>

        </div>
    </div>

    <% @active_tab = 0 %>
    <%= render partial: 'events/user_events' %>

</div>
